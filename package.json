{
  "name": "realtime-chat-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "swagger:generate": "ts-node src/shared/swagger/swagger.setup.ts",
    "start": "npm run swagger:generate && npm run build && node dist/index.js",
    "dev": "npm run swagger:generate && nodemon --ignore 'public/' src/index.ts",
    "migrate": "ts-node scripts/migrate.ts",
    "migrate:prod": "npm run build && node dist/scripts/migrate.js",
    "debug": "node --inspect src/index.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/socket.io": "^3.0.1",
    "@types/swagger-ui-express": "^4.1.8",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "humps": "^2.0.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/camelcase-keys": "^4.1.0",
